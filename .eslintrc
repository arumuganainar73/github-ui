{
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:jsx-a11y/recommended",
        "prettier"
    ],
    "rules": {
        "no-console": "warn"
    },
    "parser": "babel-eslint",
    "plugins": ["import", "jsx-a11y"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
